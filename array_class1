#include<iostream>
using namespace std;
int main()
{
	int arr[5]={2,3,4,5,6};    //array declecration
	cout<<"value at 0 index is:- "<<arr[0]<<endl;  // accesing array element
	cout<<"value at 4th index is:- "<<arr[4]<<endl;
	cout<<"value at 10th index is:- "<<arr[10]<<endl;  //accesing array over the array size
return 0;
}
